{
  "spring": {
    "application": {
      "name": "eureka-Passenger-service"
    },
    "eureka": {
      "Client": {
        "serviceUrl": "http://localhost:8761/eureka/",
        "ShouldRegisterWithEureka": true,
        "ShouldFetchRegistry": false
      },
      "instance": {
        "hostname": "localhost",
        "port": 5000
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}

